<AnchorPane xmlns="http://javafx.com/javafx/10.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="application.SokobanController">
   <children>
      <Rectangle arcHeight="5.0" arcWidth="5.0" fill="#7aaad7" height="379.0" stroke="BLACK" strokeType="INSIDE" width="640.0" />
