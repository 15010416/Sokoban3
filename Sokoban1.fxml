      <Button fx:id="Previous" layoutX="14.0" layoutY="340.0" mnemonicParsing="false" onAction="#previouslevel" text="Previous Puzzle" />
      <Label layoutX="295.0" layoutY="39.0" prefHeight="17.0" prefWidth="62.0" text="Sokoban" />
      <TextField fx:id="CurrentLevel" layoutX="320.0" layoutY="340.0" onAction="#Showcurrentlevel" text="Current Level: " />
      <TextField fx:id="Noofmoves" layoutX="477.0" layoutY="340.0" onAction="#Shownoofmoves" text="Number of Moves:" />
    <MenuBar layoutY="2.0">
      <menus>
        <Menu mnemonicParsing="false" text="File">
          <items>
            <MenuItem mnemonicParsing="false" text="New" />
            <MenuItem mnemonicParsing="false" text="Open…" />
            <Menu mnemonicParsing="false" text="Open Recent" />
            <SeparatorMenuItem mnemonicParsing="false" />
